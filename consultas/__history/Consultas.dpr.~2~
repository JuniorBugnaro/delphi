program Consultas;

uses
  Vcl.Forms,
  UnitPrincipal in 'UnitPrincipal.pas' {opcoes},
  unitDM in 'unitDM.pas' {DM: TDataModule};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(Topcoes, opcoes);
  Application.CreateForm(TDM, DM);
  Application.Run;
end.
